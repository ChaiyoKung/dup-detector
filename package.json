{
  "name": "dup-detector",
  "module": "main.ts",
  "type": "module",
  "scripts": {
    "start": "bun ./dist/main.js",
    "dev": "bun run ./src/main.ts",
    "dev:watch": "bun --watch run ./src/main.ts",
    "build": "bun build ./src/main.ts --outdir ./dist --target node --minify",
    "prod": "bun run build && bun start",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "format": "prettier ./src --write"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "file-system-cache": "^2.4.7"
  }
}
